public class BooleanTest
{
	public static void main(String[] args)
	{
		boolean b1=true;
		boolean b2=false;
		String s1= b1 + "呵呵";

		System.out.println(s1);
		System.out.println(b1);
		System.out.println(b2);
		
		//直接把字符串赋值给boolean类型变量
		//编译时会报错 不兼容的类型：String无法转换为boolean
		//b2="true";
		
	}
}
